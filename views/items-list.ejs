<h1><%= title %></h1>
<a href="/items/add">Add New</a>
<table border="1">
  <tr><th>ID</th><th>Name</th><th>Price</th><th>Actions</th></tr>
  <% items.forEach(item => { %>
    <tr>
      <td><%= item.id %></td>
      <td><%= item.name %></td>
      <td><%= item.price %></td>
      <td>
        <a href="/items/edit/<%= item.id %>">Edit</a>
        <a href="/items/delete/<%= item.id %>">Delete</a>
      </td>
    </tr>
  <% }) %>
</table>
